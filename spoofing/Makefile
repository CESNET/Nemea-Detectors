CC=gcc
CXX=g++
CFLAGS=-O2 -std=c99
CXXFLAGS=-O2
LDFLAGS=
LDLIBS=-ltrap -lcommlbr -lrt


# if no special flags or libraries are needed, it's sufficient to add the name
# of a new module here (when no rule is found, make uses default one)
MODULES=spoofing
all: ${MODULES}

doxygen:
	doxygen transitfilter.c


clean:
	rm -f *.o ${MODULES}

